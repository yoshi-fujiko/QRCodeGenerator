{
	"manifest_version": 2,
	"name": "QR Code Generator",
	"version": "0.0.0",
	"description": "表示中のウェブサイトのQRコードを表示します。おすすめQRリーダーアプリもあります。",
	"permissions": [
		"tabs",
		"*://*/*"
	],
	"icons": {
		"128": "icon/icon128.png",
		"48": "icon/icon48.png"
	},
	"browser_action": {
		"default_icon": "icon/icon.png",
		"default_popup": "popup.html",
		"default_title": "QR Code Generator"
	}
}